.detail{
    width: 100%;
}
.gudingtxt{
    font-size: 45rpx;
    text-shadow: 0 1px 0 #ccc,0 2px 0 #c9c9c9,0 3px 0 #bbb,0 4px 0 #b9b9b9,0 5px 0 #aaa,0 6px 1px rgba(0,0,0,.1),0 0 5px rgba(0,0,0,.1),0 1px 3px rgba(0,0,0,.3),0 3px 5px rgba(0,0,0,.2),0 5px 10px rgba(0,0,0,.25),0 10px 10px rgba(0,0,0,.2),0 20px 20px rgba(0,0,0,.15);
}
.detail_img{
    padding: 20rpx;
    background: #ffffff;
    margin-top: 10rpx;
}
.detail_img image{
    width: 100%;
}
.download{
    display: table;
    margin: 20rpx auto;
    border-radius: 30px;
    padding: 10rpx 150rpx;
    font-size: 32rpx;
    background: #8fce44;
    color: #2997f7;
}
.download:active{
    background: #d02476;
}
.shareQzone{
    font-size: 36rpx;
    padding: 30rpx;
}
.shareQzone button{
    display: table;
    margin: 20rpx auto;
    border-radius: 20px;
    padding: 20rpx 35%;
    font-size: 26rpx;
    background: #Ffd44d;
    
    color: #ffffff;
    
    display: inline;
    padding: 0;
    
}
.shareqq{
    display: table;
    margin: 20rpx auto;
    border-radius: 30px;
    padding: 10rpx 150rpx;
    font-size: 32rpx;
    background: #8fce44;
    color: #2997f7;
}
.like {
    font-size: 26rpx;
    padding: 20rpx;
    
}
.like text {
    margin-right: 10rpx;
    color: #d02476;
}
.detail_msg {
    padding: 20rpx;
    
}
.detail_msg text{
    line-height: 56rpx;
}
.time_and_star{
    padding: 20rpx;
}
.detail_album{
    padding: 20rpx;
    font-size: 30rpx;
    color: #666;
}
.detail_album navigator{
    color: #18a52a;
    display: inline-block;
}

.recomm_album{
    margin: 30rpx 0 100px;
    background: #ffffff;
    padding: 0 30rpx;
    border-bottom: 1px solid #ececec;
}
.recomm_album > view{
    border-bottom: 1px solid #ececec;
}
.recomm_album > view:last-child{
    border-bottom: none;
}
.recomm_tips{
    font-size: 32rpx;
    line-height: 120rpx;
}
.recomm_item{
    width: 100%;
}
.recomm_item navigator{
    display: flex;
    width: 100%;
    padding: 10rpx 0;
}
.recomm_img{
    height: 100rpx !important;
   /* flex: 1; */
    overflow: hidden;
    width: 100rpx !important;
}
.recomm_img > image{
    width: 100% !important;
    height: 100% !important;
}
.recomm_text{
    flex: 1;
    padding-left: 20rpx;
}
.recomm_text > text{
    display: block;
    font-size: 35rpx;
    color: #2b2b2b;
    padding-bottom: 15rpx;
    display: -webkit-box;
    text-overflow: ellipsis;
    overflow: hidden;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}

.recomm_text > view{
    display: flex;
    color: #000000;
    font-size: 30rpx;
    line-height: 50rpx;
}
.recomm_text > view >view{
    width: 50rpx;
    height: 50rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100%;
}